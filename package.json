{
  "engines": {
    "node": ">=20.6.0"
  },
  "name": "onlyfrogs-stylesync-server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "type-check": "tsc",
    "build": "rimraf dist && tsc --noEmit false && tsc-alias",
    "start": "node src/index.ts",
    "dev": "NODE_ENV=development tsx watch src/index",
    "format": "npx prettier --write .",
    "prettier": "prettier --write \"**/*.{js,json,md}\"",
    "precommit": "npm run lint && npm run prettier && npm run build && lint-staged",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@types/sequelize": "^4.28.20",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0"
  },
  "dependencies": {
    "@vercel/postgres": "^0.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mysql2": "^3.10.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
